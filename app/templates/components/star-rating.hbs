{{#each this.stars as |star|}}
  <span
    class="rating-star"
    role="button"
    {{!-- template-lint-disable no-implicit-this --}}
    onclick={{action onClick star.rating}}
  >
    {{#if star.isFull}}
      <i class="fas fa-star"></i>
    {{else}}
      <i class="far fa-star"></i>
    {{/if}}
  </span>
{{/each}}
